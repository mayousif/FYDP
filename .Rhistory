x = 0.5,
y = 1,
showlegend = F
)
fig <- fig %>% add_annotations(
x= 0.5,
y= 1,
xref = "paper",
yref = "paper",
text = "<b>paper reference = [0.5, 1]</b>",
showarrow = F
)
fig <- fig %>% add_annotations(
x= 0.5,
y= 1,
xref = "x",
yref = "y",
text = "x + y reference = [0.5, 1]",
showarrow = T,
ax = 20,
ay = -40
)
fig <- fig %>% layout(
xaxis = list(zeroline = F),
yaxis = list(zeroline = F)
)
fig
